@media (max-width: 800x) {
    .main-menu {
      flex-direction: column;
    }
  
    .submenu {
      position: static;
      width: 100%;
    }
  
    .menu-item a {
      padding: 10px;
      font-size: 14px;
    }
  
    .submenu-item a {
      padding: 8px 15px;
      font-size: 14px;
    }
  }

